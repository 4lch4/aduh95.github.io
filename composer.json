{
  "name": "aduh95/resume",
  "description": "My résumé",
  "type": "project",
  "license": "CC-BY-NC-ND-4.0",
  "authors": [
    {
      "name": "Antoine du HAMEL",
      "email": "duhamelantoine1995@gmail.com"
    }
  ],
  "require": {
    "aduh95/html-generator": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "aduh95\\Resume\\": "src/class"
    },
    "files": ["config/bootstrap.php", "src/middleware/bootstrap.php"]
  },
  "scripts": {
    "post-create-project-cmd": [
      "yarn install",
      "typings install",
      "echo \"Project is ready!\"",
      "echo \"You can now run the `gulp` command to get started.\""
    ],
    "pre-install-cmd": ["yarn install"],
    "pre-update-cmd": ["yarn upgrade"],
    "one-file": ["gulp clean-uglify", "gulp uglify", "gulp one-file"]
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0"
  }
}
